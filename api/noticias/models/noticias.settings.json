{
  "kind": "collectionType",
  "collectionName": "noticias",
  "info": {
    "name": "noticias"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "datetime"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "link": {
      "type": "string"
    },
    "youtube": {
      "type": "string"
    },
    "hide_date": {
      "type": "boolean"
    }
  }
}
